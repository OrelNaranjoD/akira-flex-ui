{
  "name": "akira-flex-ui",
  "version": "0.2.1",
  "scripts": {
    "lint": "nx run-many --target=lint --all --fix",
    "lint:l": "nx lint akira-flex-landing --fix",
    "lint:p": "nx lint akira-flex-platform --fix",
    "lint:t": "nx lint akira-flex-tenant --fix",
    "lint:lib": "nx lint akira-flex-core --fix",
    "start": "nx run-many --target=serve --all",
    "start:l": "nx serve akira-flex-landing",
    "start:p": "nx serve akira-flex-platform",
    "start:t": "nx serve akira-flex-tenant",
    "build": "nx run-many --target=build --all --configuration=production",
    "build:l": "nx build akira-flex-landing --configuration=production",
    "build:p": "nx build akira-flex-platform --configuration=production",
    "build:t": "nx build akira-flex-tenant --configuration=production",
    "build:lib": "nx build akira-flex-core --configuration=production",
    "test": "nx run-many --target=test --all --watch=false --browsers=ChromeHeadless",
    "test:l": "nx test akira-flex-landing --watch=false --browsers=ChromeHeadless",
    "test:p": "nx test akira-flex-platform --watch=false --browsers=ChromeHeadless",
    "test:t": "nx test akira-flex-tenant --watch=false --browsers=ChromeHeadless",
    "test:lib": "nx test akira-flex-core --watch=false --browsers=ChromeHeadless",
    "test:cov": "nx run-many --target=test --all --watch=false --browsers=ChromeHeadless --code-coverage",
    "prepare": "husky",
    "commitlint": "commitlint --edit",
    "pre-commit": "lint-staged"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.1",
    "@angular/common": "^20.3.1",
    "@angular/compiler": "^20.3.1",
    "@angular/core": "^20.3.1",
    "@angular/forms": "^20.3.1",
    "@angular/platform-browser": "^20.3.1",
    "@angular/platform-server": "^20.3.1",
    "@angular/router": "^20.3.1",
    "@angular/ssr": "^20.3.2",
    "@ngrx/store": "^20.0.1",
    "@primeuix/themes": "^1.2.3",
    "@tailwindcss/postcss": "^4.1.13",
    "express": "^5.1.0",
    "postcss": "^8.5.6",
    "primeicons": "^7.0.0",
    "primeng": "^20.1.2",
    "rxjs": "~7.8.2",
    "tailwindcss": "^4.1.13",
    "tailwindcss-primeui": "^0.6.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.2",
    "@angular-devkit/core": "^20.3.2",
    "@angular-devkit/schematics": "^20.3.2",
    "@angular/build": "^20.3.2",
    "@angular/cli": "^20.3.2",
    "@angular/compiler-cli": "^20.3.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "~9.36.0",
    "@ngrx/store": "^20.0.1",
    "@nx/angular": "^21.5.3",
    "@nx/devkit": "21.5.3",
    "@nx/eslint-plugin": "21.5.3",
    "@nx/js": "21.5.3",
    "@nx/vite": "21.5.3",
    "@nx/web": "21.5.3",
    "@nx/workspace": "21.5.3",
    "@schematics/angular": "^20.3.2",
    "@swc-node/register": "~1.11.1",
    "@swc/core": "~1.13.5",
    "@swc/helpers": "~0.5.17",
    "@types/express": "^5.0.3",
    "@types/express-serve-static-core": "^5.0.7",
    "@types/jasmine": "~5.1.9",
    "@types/node": "24.5.2",
    "@typescript-eslint/utils": "^8.44.0",
    "angular-eslint": "20.3.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsdoc": "^60.0.0",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jasmine-core": "~5.10.0",
    "jiti": "2.5.1",
    "jsdom": "~27.0.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^16.1.6",
    "nx": "21.5.3",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.44.0",
    "vite": "^7.1.6",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,js,html,css,scss,json,md}": ["eslint --fix", "prettier --write"]
  },
  "packageManager": "npm@11.6.0"
}
